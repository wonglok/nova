{
  "name": "nova",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "sst start",
    "build": "sst build",
    "deploy": "npm run stage; npm run prod; sst start",
    "frontend": "cd web; npm run build; cd ../;",
    "prod": "npx sst deploy --stage prod",
    "stage": "npx sst deploy --stage stage",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "testjs": "sst bind -- vitest run"
  },
  "devDependencies": {
    "aws-cdk-lib": "2.50.0",
    "@serverless-stack/cli": "^1.18.4",
    "@serverless-stack/resources": "^1.18.4",
    "typescript": "^4.9.3",
    "@tsconfig/node16": "^1.0.3",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@serverless-stack/node": "^1.18.4"
  },
  "workspaces": [
    "services"
  ]
}